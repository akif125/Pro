<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.akif_.pro.ProfileRider"
    android:background="@color/button_material_dark">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txuserid"
        android:layout_marginTop="134dp"
        android:text="new text"
        android:textSize="25dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/txname"
        android:layout_below="@+id/txuserid"
        android:layout_alignParentStart="true"
        android:layout_marginTop="39dp"
        android:textSize="25dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/txemail"
        android:layout_below="@+id/txname"
        android:layout_alignParentStart="true"
        android:layout_marginTop="55dp"
        android:textSize="25dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/txgender"
        android:layout_below="@+id/txemail"
        android:layout_alignParentStart="true"
        android:layout_marginTop="44dp"
        android:textSize="25dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/txcontactno"
        android:layout_below="@+id/txgender"
        android:layout_alignParentStart="true"
        android:layout_marginTop="40dp"
        android:textSize="25dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/txcnic"
        android:layout_below="@+id/txcontactno"
        android:layout_alignParentStart="true"
        android:layout_marginTop="35dp"
        android:textSize="25dp" />
</RelativeLayout>
